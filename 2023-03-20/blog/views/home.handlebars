<h1>Naujausi įrašai</h1>

<div class="row">
    {{#each posts}}
        <div class="col-4">
            <a href="/single-post/{{@key}}">
                <img src="{{photo}}" alt="{{title}}" />
            </a>
            <h3>{{title}}</h3>
            <p>{{excerpt}}</p>
        </div>
    {{/each}}
</div>
